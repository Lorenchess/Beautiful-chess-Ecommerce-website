{
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "bootstrap": "^5.0.2"
  },
  "name": "chess-ecommerce-site",
  "version": "1.0.0",
  "main": "index.html",
  "devDependencies": {
    "lite-server": "^2.6.1",
    "node-sass": "^6.0.1",
    "onchange": "^7.0.0",
    "parallelshell": "3.0.1"
  },
  "scripts": {
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\"",
    "start": "npm run watch:all"
  },
  "author": "Ramon Lorente",
  "license": "ISC",
  "description": "Ramon Lorente Portfolio Project fictional chess store"
}
